(define (sum-of-triplets sum n)
        (map (lambda (x) (if (= (sum-of-list x) sum) x null))
	(accumulate append
            null
            (map (lambda (i)
                   (map (lambda (j) (map (enumerate-interval (- j 1)))
                        )
                   )
                   (enumerate-interval 1 (- i 1))
                 )
                 (enumerate-interval 1 (- n 2))
             )
        )
        )
)