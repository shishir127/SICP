(define (repeated proc n)
	(define (iter count cumulative-proc)
		(if (= n 1)
	    	    (lambda (x) (cumulative-proc x))
		    (iter (- count 1)
		    	  (lambda (x) (proc(cumulative-proc x))
			  )
		    )
		)
	)
	(if (> n 0)
	    (iter n proc)
	    (error "Input should be greater than zero")
	)
)

> ((repeated square 2) 5)
625